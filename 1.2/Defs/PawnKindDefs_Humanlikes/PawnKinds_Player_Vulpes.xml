<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef Abstract="True" Name="BaseVulpesPawnKind">
    <race>Vulpes</race>
	<backstoryFiltersOverride>
        <li>
            <categories>
                <li>VulpesBackstoriesChildhood</li>
				<li>VulpesBackstoriesAdulthood</li>
            </categories>
        </li>
    </backstoryFiltersOverride>
    <combatPower>30</combatPower>
	<baseRecruitDifficulty>0.75</baseRecruitDifficulty>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
  </PawnKindDef>

  <PawnKindDef ParentName="BaseVulpesPawnKind">
    <defName>VulpesTribalPawn</defName>
    <label>member</label>
    <race>Vulpes</race>
    <defaultFactionType>PlayerVulpesTribal</defaultFactionType>
    <apparelTags>
      <li>Neolithic</li>
    </apparelTags>
    <apparelMoney>200~1200</apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
  </PawnKindDef>
  
  <PawnKindDef ParentName="BaseVulpesPawnKind">
    <defName>VulpesIndustrialPawn</defName>
    <label>member</label>
    <race>Vulpes</race>
    <defaultFactionType>PlayerVulpesIndustrial</defaultFactionType>
    <apparelTags>
      <li>Industrial</li>
    </apparelTags>
    <apparelMoney>200~2000</apparelMoney>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
  </PawnKindDef>

</Defs>
