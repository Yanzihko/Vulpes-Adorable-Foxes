<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<RulePackDef>
        <defName>VulpesNameM</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[FirstM] '[SecondM]' [Ident]</li>
            </rulesStrings>
            <rulesRaw>
                <li Class="Rule_File">
                    <keyword>FirstM</keyword>
                    <path>VulpesNames/VulpesMaleFirstName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>SecondM</keyword>
                    <path>VulpesNames/VulpesMaleMidName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>Ident</keyword>
                    <path>VulpesNames/VulpesSurName</path>
                </li>
            </rulesRaw>
        </rulePack>
    </RulePackDef>
    
